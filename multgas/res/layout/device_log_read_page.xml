<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/normal_background"
    tools:context="com.bohua.multgas.DeviceLogReadPage"
    tools:ignore="MergeRootFrame" >

    <Button
        android:id="@+id/DeviceLogReadPage_button_return"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="80dp"
        android:layout_height="67dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background = "@drawable/returnbutton"
        android:layout_marginTop="38dp"
        android:textSize="14sp"
        android:text="@string/button_return" />
    
    
    <ListView
        android:id="@+id/DeviceLogReadPage_listView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="110dp" >
    </ListView>

    <TextView
        android:id="@+id/DeviceLogReadPage_testview_battery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:text=""
        android:textSize="15sp" />
      
     <TextView
        android:id="@+id/DeviceLogReadPage_textView_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:text="" 
        android:textSize="15sp"/> 
    
    
</RelativeLayout>

